    "from pydantic import BaseModel\n",
    "from typing import List\n",
    "\n",
    "class User(BaseModel):\n",
    "    name: str\n",
    "    age: int\n",
    "    skills: List[str]\n",
    "\n",
    "data = {\n",
    "    \"name\": \"Udit\",\n",
    "    \"age\": 26,\n",
    "    \"skills\": [\"Money\"]\n",
    "}\n",
    "\n",
    "user = User(**data)\n",
    "print(user)\n"
   ]
  }
